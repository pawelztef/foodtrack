    <%= f.input :address_line1, label: false, placeholder: 'Ulica' %> 
    <%= f.input :town, label: false, placeholder: 'Miasto' %> 
    <%= f.input :code, label: false, placeholder: 'Kod pocztowy', input_html: { class: 'postal-code'} %> 
    <%= f.input :date, label: false, as: :string, placeholder: 'Data przyjazdu', input_html: { class: 'track-date'} %> 
    <%= f.input :date_out, label: false, as: :string, placeholder: 'Data odjazdu', input_html: { class: 'track-date'} %> 
    <%= f.input :catchphrase, label: false, as: :string, placeholder: 'Wyimek', input_html: { class: 'stop-catchphrase'} %> 
    <%= link_to_remove_association "#{fa_icon('times')} UsuÅ„".html_safe, f, class: 'btn btn-default' %>
